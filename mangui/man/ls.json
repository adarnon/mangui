{
    "name": "ls - list directory contents",
    "synopsis": [
        "ls [OPTION]... [FILE]..."
    ],
    "description": "List  information  about  the FILEs (the current directory by default).\nSort entries alphabetically if none of -cftuvSUX nor --sort.\nMandatory arguments to long options are  mandatory  for  short  options\ntoo.",
    "options": [
        {
            "cmds": [
                {
                    "name": "-a",
                    "type": "binary"
                },
                {
                    "name": "--all",
                    "type": "binary"
                }
            ],
            "desc": "do not ignore entries starting with ."
        },
        {
            "cmds": [
                {
                    "name": "-A",
                    "type": "binary"
                },
                {
                    "name": "--almost-all",
                    "type": "binary"
                }
            ],
            "desc": "do not list implied . and .."
        },
        {
            "cmds": [
                {
                    "name": "--author",
                    "type": "binary"
                }
            ],
            "desc": "with -l, print the author of each file"
        },
        {
            "cmds": [
                {
                    "name": "-b",
                    "type": "binary"
                },
                {
                    "name": "--escape",
                    "type": "binary"
                }
            ],
            "desc": "print octal escapes for nongraphic characters"
        },
        {
            "cmds": [
                {
                    "name": "--block-size",
                    "metavar": "SIZE",
                    "type": "str"
                }
            ],
            "desc": "use SIZE-byte blocks"
        },
        {
            "cmds": [
                {
                    "name": "-B",
                    "type": "binary"
                },
                {
                    "name": "--ignore-backups",
                    "type": "binary"
                }
            ],
            "desc": "do not list implied entries ending with ~"
        },
        {
            "cmds": [
                {
                    "name": "-c",
                    "type": "binary"
                }
            ],
            "desc": "with -lt: sort by, and show, ctime (time of last modification of\nfile status information) with -l: show ctime and  sort  by  name\notherwise: sort by ctime"
        },
        {
            "cmds": [
                {
                    "name": "-C",
                    "type": "binary"
                }
            ],
            "desc": "list entries by columns"
        },
        {
            "cmds": [
                {
                    "name": "--color[",
                    "metavar": "WHEN]",
                    "type": "str"
                }
            ],
            "desc": "control  whether  color is used to distinguish file types.  WHEN\nmay be `never', `always', or `auto'"
        },
        {
            "cmds": [
                {
                    "name": "-d",
                    "type": "binary"
                },
                {
                    "name": "--directory",
                    "type": "binary"
                }
            ],
            "desc": "list directory entries instead of contents, and do not  derefer\u2010\nence symbolic links"
        },
        {
            "cmds": [
                {
                    "name": "-D",
                    "type": "binary"
                },
                {
                    "name": "--dired",
                    "type": "binary"
                }
            ],
            "desc": "generate output designed for Emacs' dired mode"
        },
        {
            "cmds": [
                {
                    "name": "-f",
                    "type": "binary"
                }
            ],
            "desc": "do not sort, enable -aU, disable -ls --color"
        },
        {
            "cmds": [
                {
                    "name": "-F",
                    "type": "binary"
                },
                {
                    "name": "--classify",
                    "type": "binary"
                }
            ],
            "desc": "append indicator (one of */=>@|) to entries"
        },
        {
            "cmds": [
                {
                    "name": "--file-type",
                    "type": "binary"
                }
            ],
            "desc": "likewise, except do not append `*'"
        },
        {
            "cmds": [
                {
                    "name": "--format",
                    "metavar": "WORD",
                    "type": "str"
                }
            ],
            "desc": "across  -x, commas -m, horizontal -x, long -l, single-column -1,\nverbose -l, vertical -C"
        },
        {
            "cmds": [
                {
                    "name": "--full-time",
                    "type": "binary"
                }
            ],
            "desc": "like -l --time-style=full-iso"
        },
        {
            "cmds": [
                {
                    "name": "-g",
                    "type": "binary"
                }
            ],
            "desc": "like -l, but do not list owner"
        },
        {
            "cmds": [
                {
                    "name": "--group-directories-first",
                    "type": "binary"
                }
            ],
            "desc": "group directories before files."
        },
        {
            "cmds": [],
            "desc": "augment with a --sort option, but any use  of  --sort=none  (-U)\ndisables grouping"
        },
        {
            "cmds": [
                {
                    "name": "-G",
                    "type": "binary"
                },
                {
                    "name": "--no-group",
                    "type": "binary"
                }
            ],
            "desc": "in a long listing, don't print group names"
        },
        {
            "cmds": [
                {
                    "name": "-h",
                    "type": "binary"
                },
                {
                    "name": "--human-readable",
                    "type": "binary"
                }
            ],
            "desc": "with -l, print sizes in human readable format (e.g., 1K 234M 2G)"
        },
        {
            "cmds": [
                {
                    "name": "--si",
                    "type": "binary"
                }
            ],
            "desc": "likewise, but use powers of 1000 not 1024"
        },
        {
            "cmds": [
                {
                    "name": "-H",
                    "type": "binary"
                },
                {
                    "name": "--dereference-command-line",
                    "type": "binary"
                }
            ],
            "desc": "follow symbolic links listed on the command line"
        },
        {
            "cmds": [
                {
                    "name": "--dereference-command-line-symlink-to-dir",
                    "type": "binary"
                }
            ],
            "desc": "follow  each  command line symbolic link that points to a direc\u2010\ntory"
        },
        {
            "cmds": [
                {
                    "name": "--hide",
                    "metavar": "PATTERN",
                    "type": "str"
                }
            ],
            "desc": "do not list implied entries matching shell  PATTERN  (overridden\nby -a or -A)"
        },
        {
            "cmds": [
                {
                    "name": "--indicator-style",
                    "metavar": "WORD",
                    "type": "str"
                }
            ],
            "desc": "append indicator with style WORD to entry names: none (default),\nslash (-p), file-type (--file-type), classify (-F)"
        },
        {
            "cmds": [
                {
                    "name": "-i",
                    "type": "binary"
                },
                {
                    "name": "--inode",
                    "type": "binary"
                }
            ],
            "desc": "print the index number of each file"
        },
        {
            "cmds": [
                {
                    "name": "-I",
                    "type": "binary"
                },
                {
                    "name": "--ignore",
                    "metavar": "PATTERN",
                    "type": "str"
                }
            ],
            "desc": "do not list implied entries matching shell PATTERN"
        },
        {
            "cmds": [
                {
                    "name": "-k",
                    "type": "binary"
                }
            ],
            "desc": "like --block-size=1K"
        },
        {
            "cmds": [
                {
                    "name": "-l",
                    "type": "binary"
                }
            ],
            "desc": "use a long listing format"
        },
        {
            "cmds": [
                {
                    "name": "-L",
                    "type": "binary"
                },
                {
                    "name": "--dereference",
                    "type": "binary"
                }
            ],
            "desc": "when showing file information for a symbolic link, show informa\u2010\ntion  for  the file the link references rather than for the link\nitself"
        },
        {
            "cmds": [
                {
                    "name": "-m",
                    "type": "binary"
                }
            ],
            "desc": "fill width with a comma separated list of entries"
        },
        {
            "cmds": [
                {
                    "name": "-n",
                    "type": "binary"
                },
                {
                    "name": "--numeric-uid-gid",
                    "type": "binary"
                }
            ],
            "desc": "like -l, but list numeric user and group IDs"
        },
        {
            "cmds": [
                {
                    "name": "-N",
                    "type": "binary"
                },
                {
                    "name": "--literal",
                    "type": "binary"
                }
            ],
            "desc": "print raw entry names (don't treat e.g. control characters  spe\u2010\ncially)"
        },
        {
            "cmds": [
                {
                    "name": "-o",
                    "type": "binary"
                }
            ],
            "desc": "like -l, but do not list group information"
        },
        {
            "cmds": [
                {
                    "name": "-p",
                    "type": "binary"
                },
                {
                    "name": "--indicator-style",
                    "metavar": "slash",
                    "type": "str"
                }
            ],
            "desc": "append / indicator to directories"
        },
        {
            "cmds": [
                {
                    "name": "-q",
                    "type": "binary"
                },
                {
                    "name": "--hide-control-chars",
                    "type": "binary"
                }
            ],
            "desc": "print ? instead of non graphic characters"
        },
        {
            "cmds": [
                {
                    "name": "--show-control-chars",
                    "type": "binary"
                }
            ],
            "desc": "show  non  graphic  characters  as-is (default unless program is\n`ls' and output is a terminal)"
        },
        {
            "cmds": [
                {
                    "name": "-Q",
                    "type": "binary"
                },
                {
                    "name": "--quote-name",
                    "type": "binary"
                }
            ],
            "desc": "enclose entry names in double quotes"
        },
        {
            "cmds": [
                {
                    "name": "--quoting-style",
                    "metavar": "WORD",
                    "type": "str"
                }
            ],
            "desc": "use quoting style WORD for entry names: literal, locale,  shell,\nshell-always, c, escape"
        },
        {
            "cmds": [
                {
                    "name": "-r",
                    "type": "binary"
                },
                {
                    "name": "--reverse",
                    "type": "binary"
                }
            ],
            "desc": "reverse order while sorting"
        },
        {
            "cmds": [
                {
                    "name": "-R",
                    "type": "binary"
                },
                {
                    "name": "--recursive",
                    "type": "binary"
                }
            ],
            "desc": "list subdirectories recursively"
        },
        {
            "cmds": [
                {
                    "name": "-s",
                    "type": "binary"
                },
                {
                    "name": "--size",
                    "type": "binary"
                }
            ],
            "desc": "print the allocated size of each file, in blocks"
        },
        {
            "cmds": [
                {
                    "name": "-S",
                    "type": "binary"
                }
            ],
            "desc": "sort by file size"
        },
        {
            "cmds": [
                {
                    "name": "--sort",
                    "metavar": "WORD",
                    "type": "str"
                }
            ],
            "desc": "sort  by  WORD  instead of name: none -U, extension -X, size -S,\ntime -t, version -v"
        },
        {
            "cmds": [
                {
                    "name": "-u",
                    "type": "binary"
                },
                {
                    "name": " access  -u",
                    "type": "binary"
                },
                {
                    "name": " use  -u",
                    "type": "binary"
                },
                {
                    "name": "ctime -c",
                    "type": "binary"
                },
                {
                    "name": "or status -c; use specified",
                    "type": "binary"
                }
            ],
            "desc": "with -l, show time as WORD instead of modification  time:  atime\ntime as sort key if --sort=time"
        },
        {
            "cmds": [
                {
                    "name": "--time-style",
                    "metavar": "STYLE",
                    "type": "str"
                }
            ],
            "desc": "with -l, show times using style STYLE: full-iso, long-iso,  iso,\nlocale,  +FORMAT.   FORMAT is interpreted like `date'; if FORMAT\nis FORMAT1FORMAT2, FORMAT1 applies to non-recent  files\nand FORMAT2 to recent files; if STYLE is prefixed with `posix-',\nSTYLE takes effect only outside the POSIX locale"
        },
        {
            "cmds": [
                {
                    "name": "-t",
                    "type": "binary"
                }
            ],
            "desc": "sort by modification time"
        },
        {
            "cmds": [
                {
                    "name": "-T",
                    "type": "binary"
                },
                {
                    "name": "--tabsize",
                    "metavar": "COLS",
                    "type": "str"
                }
            ],
            "desc": "assume tab stops at each COLS instead of 8"
        },
        {
            "cmds": [
                {
                    "name": "-u",
                    "type": "binary"
                }
            ],
            "desc": "with -lt: sort by, and show, access time with  -l:  show  access\ntime and sort by name otherwise: sort by access time"
        },
        {
            "cmds": [
                {
                    "name": "-U",
                    "type": "binary"
                }
            ],
            "desc": "do not sort; list entries in directory order"
        },
        {
            "cmds": [
                {
                    "name": "-v",
                    "type": "binary"
                }
            ],
            "desc": "natural sort of (version) numbers within text"
        },
        {
            "cmds": [
                {
                    "name": "-w",
                    "type": "binary"
                },
                {
                    "name": "--width",
                    "metavar": "COLS",
                    "type": "str"
                }
            ],
            "desc": "assume screen width instead of current value"
        },
        {
            "cmds": [
                {
                    "name": "-x",
                    "type": "binary"
                }
            ],
            "desc": "list entries by lines instead of by columns"
        },
        {
            "cmds": [
                {
                    "name": "-X",
                    "type": "binary"
                }
            ],
            "desc": "sort alphabetically by entry extension"
        },
        {
            "cmds": [
                {
                    "name": "-Z",
                    "type": "binary"
                },
                {
                    "name": "--context",
                    "type": "binary"
                }
            ],
            "desc": "print any SELinux security context of each file"
        },
        {
            "cmds": [
                {
                    "name": "-1",
                    "type": "binary"
                }
            ],
            "desc": "list one file per line"
        },
        {
            "cmds": [
                {
                    "name": "--help",
                    "type": "binary"
                }
            ],
            "desc": "display this help and exit"
        },
        {
            "args": [
                {
                    "name": "--version",
                    "type": "binary"
                }
            ],
            "desc": "output version information and exit"
        }
    ]
}
